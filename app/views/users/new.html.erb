<div class='row'>


  <div class='col-md-4'>

  </div>

  <div class='col-md-4'>

    <%= form_for(@user) do |f| %>

      <%= f.label :username %>
      <%= f.text_field :username %>

      <%= f.label :email %>
      <%= f.text_field :email %>

      <%= f.label :password %>
      <%= f.password_field :password %>

      <%= f.label :password_confirmation %>
      <%= f.password_field :password_confirmation %>

      <%= f.submit "Create Account" %>

    <% end %>

  </div>

  <div class='col-md-4'>

  </div>

</div>